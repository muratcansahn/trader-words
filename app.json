{
  "expo": {
    "name": "FinansSözlük",
    "slug": "trader-words",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#151718"
      },
      "package": "com.muratcansahn.traderwords"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/yatirimx-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#151718"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c2a44dc8-00b5-4288-b658-ad2bf14cb481"
      }
    }
  }
}
